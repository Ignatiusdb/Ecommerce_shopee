        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">User list</h2>
                <!-- <div>
                    <a href="#" class="btn btn-primary"><i class="material-icons md-plus"></i> Create new</a>
                </div> -->
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <!-- <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div> -->
                        
                </header>
                <div class="card-body">
                    <div class="container-fluid">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-sm borrder-2 text-center">
                                <thead>
                                    <tr>
                                        <th scope="col">S.No</th>
                                        <th scope="col">Name</th>
                                        <!-- <th scope="col">lastName</th> -->
                                        <th scope="col">Email</th>
                                        <!-- <th scope="col">Password</th> -->
                                        <!-- <th scope="col">Gender</th>
                                        <th scope="col">Phone Number</th> -->
                                        <th scope="col">Status</th>
                                            
                                    </tr>
                                </thead>
                                <tbody>
                                    <% userData?.forEach((element, index) => { %>
                                        <tr class="align-middle">
                                            <td><%= index + 1 %></td>
                                            <td><%= element.name %></td>
                                            <td><%= element.email %></td>
                                            <td>
                                                <form action="/admin/users/block-user/<%= element._id %>" method="POST" onsubmit="return confirmBlockUser()">
                                                    <button type="submit" class="btn btn-<%= element.isBlocked ? 'danger' : 'success' %> btn-small">
                                                        <%= element.isBlocked ? 'Unblock' : 'Block' %>
                                                        <i class="fa-solid fa-<%= element.isBlocked ? 'lock' : 'unlock' %>" style="color: <%= element.isBlocked ? '#d7dce5' : '#e9ecf1' %>; margin-left: 3px;"></i>
                                                    </button>
                                                </form>
                                            </td>
                                            
                                        </tr>
                                        
                                    <script>
                                        function confirmBlockUser() {
                                            return confirm("Are you sure you want to block/unblock this user?");
                                        }
                                    </script>
                                    <% }) %>
                                </tbody>
                                
                            </table>
                        </div>
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
         
        </section> <!-- content-main end// -->
